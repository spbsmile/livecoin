<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Description>Client for Livecoin bitcoin exchange API.</Description>
    <Authors>Makar Zabiyakin</Authors>
    <PackageId>LivecoinNet</PackageId>
    <PackageProjectUrl>https://github.com/spbsmile/livecoin</PackageProjectUrl>
    <RepositoryUrl>https://github.com/spbsmile/livecoin</RepositoryUrl>
    <PackageTags>livecoin</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/spbsmile/livecoin/master/livecoin_logo.png</PackageIconUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/spbsmile/livecoin/master/LICENSE</PackageLicenseUrl>
    <PackageReleaseNotes>
    </PackageReleaseNotes>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>
</Project>